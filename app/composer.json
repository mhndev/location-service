{
    "name": "mhndev/location-service",
    "description": "Location Service",
    "keywords": ["location service"],
    "homepage": "http://github.com/mhndev/location-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Majid Abdolhosseini",
            "email": "majid@mhndev.com",
            "homepage": "http://www.mhndev.com/"
        }
    ],
    "autoload": {
      "psr-4": {
        "mhndev\\locationService\\": "src/app"
      }
    },

    "require": {
        "php": ">=5.6.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "mhndev/location": "1.*",
        "mhndev/rest-hal": "dev-develop",
        "mhndev/media":"~0.0",
        "mhndev/slim-file-response" :"~0.1.0",
        "mhndev/php-std": "dev-develop",
        "vlucas/phpdotenv": "^2.4",
        "tuupola/cors-middleware": "^0.5.2",
        "phpoffice/phpexcel" : "*",
        "league/geotools": "^0.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable": true
}
